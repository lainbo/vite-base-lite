{
  "name": "vite-base",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite --port 56789 --open",
    "build": "vue-tsc --noEmit && vite build",
    "clear": "rimraf node_modules && pnpm i",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit",
    "stable-up": "taze -w -I",
    "up": "ncu -i"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.2",
    "dayjs": "^1.11.10",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.0.4",
    "pinia": "^2.1.7",
    "vue": "^3.4.14"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "@iconify/json": "^2.2.170",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.5",
    "@unocss/eslint-plugin": "^0.58.3",
    "@unocss/preset-rem-to-px": "^0.58.3",
    "@unocss/reset": "^0.58.3",
    "@vitejs/plugin-vue": "^5.0.3",
    "eslint": "^8.56.0",
    "sass": "^1.69.7",
    "stylelint": "^16.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "typescript": "^5.3.3",
    "unocss": "^0.58.3",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.11",
    "vite-plugin-vue-devtools": "^7.0.10",
    "vue-tsc": "^1.8.27"
  }
}
