{
  "name": "vite-base",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite --port 56789 --open",
    "build": "vue-tsc --noEmit && vite build",
    "clear": "rimraf node_modules && pnpm i",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit",
    "stable-up": "taze -w -I",
    "up": "taze major -I"
  },
  "dependencies": {
    "@vueuse/core": "^9.13.0",
    "dayjs": "^1.11.7",
    "lodash-es": "^4.17.21",
    "nanoid": "^4.0.1",
    "pinia": "^2.0.33",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.36.0",
    "@iconify/json": "^2.2.35",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.15.3",
    "@unocss/preset-rem-to-px": "^0.50.4",
    "@unocss/preset-web-fonts": "^0.50.4",
    "@unocss/reset": "^0.50.4",
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.4",
    "sass": "^1.59.3",
    "taze": "^0.9.0",
    "typescript": "^4.9.5",
    "unocss": "^0.50.4",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-define-options": "^1.2.4",
    "vite": "^4.1.4",
    "vue-tsc": "^1.2.0"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
